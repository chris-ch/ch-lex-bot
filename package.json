    "@mistralai/mistralai": "^1.6.0",
    "aws-amplify": "^6.14.3",
    "jwt-decode": "^4.0.0",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.5.13",
    "vue-i18n": "^9.14.4",
    "vue-router": "^4.5.1",
    "vuetify": "3.7.0-beta.1"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.0",
    "@aws-amplify/backend-cli": "^1.7.0",
