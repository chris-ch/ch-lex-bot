    "aws-cdk-lib": "2.180.0",
    "constructs": "^10.4.2",
    "cypress": "^13.17.0",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^6.2.6",
